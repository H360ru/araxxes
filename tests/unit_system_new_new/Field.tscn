[sub_resource type="GDScript" id=1]
script/source = "extends Node

var robot:PackedScene = preload(\"res://tests/unit_system_new_new/Unit1.tscn\")

onready var units:UnitsGroup = $\"../Units\"
onready var grid:NavigationGrid = $\"../Grid\"
onready var player:Player = $\"../Player\"

enum States{
	WAIT_PLAYER,
	READY
}

var state:int

func SReady():
	state = States.READY
	
func SWaitPlayer():
	state = States.WAIT_PLAYER

func _ready():
	SReady()

func _process(delta):
	var mouse_cell = grid.world_to_map(grid.get_local_mouse_position())
	match state:
		States.READY:
			if Input.is_action_just_pressed(\"mouse_left\"):
				var unit = units.get_unit_on_cell(mouse_cell)
				if unit:
					player.unit = unit
				elif player.unit != null:
					player.clear_unit_path()
					var path = grid.find_pixel_path(player.unit.map_position, mouse_cell)
					for i in path:
						player.add_unit_path_point(i)
				
			if Input.is_action_just_pressed(\"ui_select\"):
				if player.unit != null:
					player.go_path()
					SWaitPlayer()

func _on_Player_move_finished():
	SReady()


func _on_Field_ready():
	units.create_unit(robot, Vector2(3, 3))
"

[sub_resource type="GDScript" id=3]
script/source = "extends Camera2D


